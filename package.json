{
  "name": "photo-gallery-site",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@9.6.0",
  "repository": {
    "type": "git",
    "url": "https://"
  },
  "scripts": {
    "build": "npm run build:manifest && tsc && vite build",
    "build:manifest": "node scripts/build-manifest.js",
    "dev": "vite",
    "format": "prettier --write \"src/**/*.ts\" ",
    "lint": "eslint --fix",
    "prepare": "simple-git-hooks",
    "serve": "vite preview",
    "test:s3": "node scripts/test-s3.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.817.0",
    "@aws-sdk/s3-request-presigner": "3.817.0",
    "@headlessui/react": "2.2.4",
    "@radix-ui/react-slot": "1.2.3",
    "@remixicon/react": "4.6.0",
    "@t3-oss/env-core": "0.13.4",
    "@tanstack/react-query": "5.77.0",
    "@types/sharp": "0.32.0",
    "blurhash": "2.0.5",
    "clsx": "2.1.1",
    "dotenv": "16.5.0",
    "es-toolkit": "1.38.0",
    "immer": "10.1.1",
    "jotai": "2.12.4",
    "masonry-layout": "4.2.2",
    "motion": "12.12.2",
    "ofetch": "1.4.1",
    "react": "19.1.0",
    "react-blurhash": "0.3.0",
    "react-dom": "19.1.0",
    "react-intersection-observer": "9.16.0",
    "react-router": "7.6.0",
    "sharp": "0.34.2",
    "sonner": "2.0.3",
    "tailwind-merge": "3.3.0",
    "usehooks-ts": "3.1.1",
    "zod": "3.25.28"
  },
  "devDependencies": {
    "@egoist/tailwindcss-icons": "1.9.0",
    "@iconify-json/mingcute": "1.2.3",
    "@innei/prettier": "^0.15.0",
    "@tailwindcss/container-queries": "0.1.1",
    "@tailwindcss/postcss": "4.1.7",
    "@tailwindcss/typography": "0.5.16",
    "@tailwindcss/vite": "4.1.7",
    "@types/node": "22.15.21",
    "@types/react": "19.1.5",
    "@types/react-dom": "19.1.5",
    "@vitejs/plugin-react": "^4.5.0",
    "autoprefixer": "10.4.21",
    "click-to-react-component": "1.1.2",
    "code-inspector-plugin": "0.20.10",
    "daisyui": "5.0.37",
    "eslint": "9.27.0",
    "eslint-config-hyoban": "4.0.7",
    "lint-staged": "16.0.0",
    "postcss": "8.5.3",
    "postcss-import": "16.1.0",
    "postcss-js": "4.0.1",
    "prettier": "3.5.3",
    "simple-git-hooks": "2.13.0",
    "tailwind-scrollbar": "4.0.2",
    "tailwind-variants": "1.0.0",
    "tailwindcss": "4.1.7",
    "tailwindcss-animated": "2.0.0",
    "typescript": "5.8.3",
    "vite": "6.3.5",
    "vite-plugin-checker": "0.9.3",
    "vite-tsconfig-paths": "5.1.4"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --ignore-path ./.gitignore --write "
    ],
    "*.{js,ts,cjs,mjs,jsx,tsx,json}": [
      "eslint --fix"
    ]
  }
}